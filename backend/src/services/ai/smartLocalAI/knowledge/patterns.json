  "entityPatterns": {
    "quantity": {
      "regex": "(\\d+(?:,\\d{3})*(?:\\.\\d+)?)\\s*(units?|pcs?|pieces?|boxes?|packs?|kg|kgs?|tons?|meters?|m|mm|cm)?",
      "type": "number",
      "examples": ["500 units", "100 kg", "50 meters"]
    },
    "productCode": {
      "regex": "([mM]\\d+(?:\\.\\d+)?|grade\\s*\\d+(?:\\.\\d+)?|class\\s*[a-zA-Z0-9]+|size\\s*\\d+)",
      "type": "string",
      "examples": ["M12", "Grade 8.8", "Class B"]
    },
    "orderNumber": {
      "regex": "(ORD|QT|INV)-?\\d{4,}",
      "type": "string",
      "examples": ["ORD-12345", "QT-5678", "INV-9999"]
    },
    "location": {
      "regex": "(johannesburg|jo'?burg|pretoria|pta|cape town|cpt|durban|dbn|gauteng|gp|western cape|kzn|kwazulu|randfontein|rustenburg|witbank|secunda|sasolburg|nelspruit|polokwane|bloemfontein|port elizabeth|pe|east london|el)",
      "type": "location",
      "examples": ["Johannesburg", "Cape Town", "Durban"]
    },
    "measurement": {
      "regex": "(\\d+(?:\\.\\d+)?)\\s*(mm|cm|m|inch|inches|ft|feet|kg|ton|kn|mpa|bar|psi|celsius|Â°c)",
      "type": "measurement",
      "examples": ["12mm", "800 MPa", "25 bar"]
    },
    "sansCode": {
      "regex": "(SANS|SABS)\\s*\\d{3,5}(?:-\\d+)?",
      "type": "compliance",
      "examples": ["SANS 1700", "SABS 1473", "SANS 10142-1"]
    }
  },
